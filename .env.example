BOT_TOKEN=your_bot_token_from_@BotFather
ADMIN_USERNAME=your_telegram_username

# Database (optional)
DATABASE_URL=

# Cobalt API Configuration
# External Cobalt worker (отдельный воркер с Cobalt)
COBALT_API_URL=http://your-cobalt-server:9000/
USE_COBALT=true
COBALT_API_KEY=your_cobalt_api_key
API_PORT=9000

# Proxy Configuration (для всех загрузок: yt-dlp и Cobalt)
# Поддерживаются: http://, https://, socks5://
# Формат с авторизацией: socks5://username:password@host:port
# Формат без авторизации: socks5://host:port
# Спецсимволы в пароле нужно кодировать: & = %26, ^ = %5E, @ = %40 и т.д.
HTTP_PROXY=socks5://user:password@proxy-host:1080
SOCKS_PROXY=socks5://user:password@proxy-host:1080

# Примеры:
# HTTPS_PROXY=socks5://user:pass@home-server-ip:1080
# HTTPS_PROXY=http://proxy.example.com:8080

# Cookies (optional, для приватных видео)
COOKIES_CONTENT=
